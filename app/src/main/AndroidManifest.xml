<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sleepgraphyapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SleepgraphyApp">
        <activity android:name=".ForgotPass"></activity>
        <activity android:name=".Login" />
        <activity android:name=".Homepage" />
        <activity android:name=".Assessment" />
        <activity android:name=".Signup" />
        <activity android:name=".ForgotPassPage" />
        <activity android:name=".SignupPage" />
        <activity android:name=".LoginPage" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>